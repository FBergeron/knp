;;; 括弧 => 固有名詞
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;; 文っぽいのはいやだ
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ]* [ * * * * (、 。 は) ] [ * * * * ^(」 』) ]* ) 
	( [特殊 * * * (」 』) ] ) 非複固 )

;; 」 の直前の用言はいやだ
;( ( [特殊 * * * (「 『) ] ) 
;	( [ * * * * ^(」 』) ]* [ (動詞 形容詞 判定詞 助動詞 接尾辞) ] ) 
;	( [特殊 * * * (」 』) ] ) 非複固 )

;; 用言を含むのはいやだ, 感動詞, 指示詞 もいやだ
;; 接尾辞?
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ]* [ (動詞 形容詞 判定詞 助動詞 感動詞 指示詞) ] [ * * * * ^(」 』) ]* ) 
	( [特殊 * * * (」 』) ] ) 非複固 )

;; 役職で終わるのは固有名詞じゃないでしょう
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ]* [ * * * * ^(」 』) ((T役職))] ) 
	( [特殊 * * * (」 』) ] ) 非複固 )

;; 「〜氏」は除く
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ]* [ * * * * 氏] ) 
	( [特殊 * * * (」 』) ] ) 非複固 )

;; A の B はいやだ (微妙)
;( ( [特殊 * * * (「 『) ] ) 
;	( [ * * * * ^(」 』) ] [ 助詞 接続助詞 * * (の) ] [ * * * * ^(」 』) ] ) 
;	( [特殊 * * * (」 』) ] ) 非複固 )

;; 非複固を与えることによって下で上書きさせない
;; 名前が「〜」 => 人名
( ( [(名詞 接尾辞) * * * (名前) ] [助詞]* 
	[特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ((^非複固 ^複固))] [ * * * * ^(」 』) ((^非複固 ^複固))]* ) 
	( [特殊 * * * (」 』)] ) 複固:人名 非複固)

;; => 地名
( ( [(名詞 接尾辞) * * * (幹線 遊園地 アパート) ] 
	[ * * * * (、 の) ]* 
	[特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ((^非複固 ^複固))] [ * * * * ^(」 』) ((^非複固 ^複固))]* ) 
	( [特殊 * * * (」 』)] ) 複固:地名 非複固)

;; ×
;; (住所)の「〜」

;; 〜機関「〜」 => 組織名
( ( [* * * * * ((意味素組織)) ] 
	[ * * * * (、 の) ]* 
	[特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ((^非複固 ^複固))] [ * * * * ^(」 』) ((^非複固 ^複固))]* ) 
	( [特殊 * * * (」 』)] ) 複固:組織名 非複固)

;; 組織が末尾に来るものは組織にする
;( ( [特殊 * * * (「 『) ] ) 
;	( [ * * * * ^(」 』) ]* [ * * * * ^(」 』) ((意味素組織))] ) 
;	( [特殊 * * * (」 』) ] ) 複固:組織名 非複固)

;; ×
;; チーム名も「〜」

;; => 固有名詞 (2 形態素以上必要)
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ((^非複固))] 
	[ * * * * ^(」 』) ((^非複固))] 
	[ * * * * ^(」 』) ((^非複固))]* ) 
	( [特殊 * * * (」 』)] ) 複固:固有名詞 )

;; => 固有名詞 (1 形態素)
( ( [特殊 * * * (「 『) ] ) 
	( [ * * * * ^(」 』) ((^非複固 カタカナ) (^非複固 &意味素:作品・出版物名) (^非複固 英記号))] )
	( [特殊 * * * (」 』)] ) 複固:固有名詞 )

