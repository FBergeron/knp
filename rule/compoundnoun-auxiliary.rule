;; 住所の後の数字
( ( [* * * * * ((住所))] ) ( [名詞 数詞] [名詞 数詞]* ) ( )
	住所 複固:地名 )

;; 人名の頭についているごみを人名から除く
;; 長男, 妻 など
( ( ) ( [(接頭辞 名詞) * * * * ((複固:人名 漢字 &意味素:人間＜親族関係＞))] ) 
	( [* * * * * ((複固:人名))] ) 
	^複固:人名)
( ( ) ( [(接頭辞 名詞) * * * (故 本名 自称 陶工 版元) ((複固:人名))] ) 
	( [* * * * * ((複固:人名))] ) 
	^複固:人名)

;; 頭についているごみを除く
( ( ) ( [* * * * (前 元 現 新 旧 駐) ((複固:人名))] )
	( [* * * * * ((複固:人名))] ) 
	^複固:人名)
( ( ) ( [* * * * (前 元 現 駐) ((複固:組織名))] )
	( [* * * * * ((複固:組織名))] ) 
	^複固:組織名)

( ( ) ( [* * * * (「 『 “ （) ((複固:人名) (複固:組織名) (複固:地名))] )
	( [* * * * * ((複固:人名) (複固:組織名) (複固:地名))] ) 
	^複固:人名 ^複固:組織名 ^複固:地名)

;; 国名の救済
( ( ) ( [ 名詞 * * * ^(日本) ((&意味素:独立国 単固:地名))] ) ( [* * * * * ((^&意味素:軍・隊))] )
	複固:地名)
( ( ) ( [ * * * * (日 米 英) ((複固:組織名) (複固:人名))] ) ( )
	複固:地名 例外:単独)
( ( ) ( [ 名詞 * * * (日本) ((^複固))] ) ( ) 
	複固:地名)

;; 大学名の救済
( ( ) ( [* * * * * ((カタカナ))] [* * * * (大 大学)] ) ( ) 
  複固:組織名)

;; ・で始まっちゃだめだよ
( ( [* * * * * ((^複固))] ) 
  ( [* * * * ・ ((複固))] )
  ( [* * * * * ((複固))] )
    ^複固)
( ( [* * * * * ((^複固:組織名 複固))] ) 
  ( [* * * * ・ ((複固:組織名))] )
  ( [* * * * * ((複固:組織名))] )
    ^複固)
( ( [* * * * * ((^複固:人名 複固))] ) 
  ( [* * * * ・ ((複固:人名))] )
  ( [* * * * * ((複固:人名))] )
    ^複固)

;; ・で終わっちゃだめだよ
( ( [* * * * * ((複固))] ) 
  ( [* * * * ・ ((複固))] )
  ( [* * * * * ((^複固))] )
    ^複固)
( ( [* * * * * ((複固:組織名))] ) 
  ( [* * * * ・ ((複固:組織名))] )
  ( [* * * * * ((^複固:組織名 複固))] )
    ^複固)
( ( [* * * * * ((複固:人名))] ) 
  ( [* * * * ・ ((複固:人名))] )
  ( [* * * * * ((^複固:人名 複固))] )
    ^複固)

;; まだ 旧, 新 が組織名についてないもの
( ( ) ( [* * * * (旧 新 国際 世界) ((^複固:組織名))] ) ( [* * * * * ((複固:組織名))] )
  複固:組織名)

;; 民族名はだめ
( ( ) ( [(名詞 接頭辞 接尾辞) * * * * ((複固))] [(名詞 接頭辞 接尾辞) * * * * ((複固))]* )
  ( [* * * * 族] )
  ^複固)

