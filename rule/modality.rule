;;
;;	□■□ ＫＮＰ文法規則 : modality.rule □■□
;;
;;	働き：構造決定後に文節へFEATURE付与
;;
;;	ルール：( (前方文節列) (自身の文節) (後方文節列) FEATURE列 )
;;			※ 中央の「文節」にFEATURE列が与えられる
;;
;;	適用方法：全ルールを前から順に，文中の後ろの文節から順に
;;			※ マッチしてもBREAKなし
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;;; 疑問 (構造をみないもの)
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

( ( ?* ) ( < ( ?* [助詞 * * * か] [特殊 * * * (。 ． ？)]* ) ((文末)) > ) ( ?* ) 疑問 )

( ( ?* ) ( < ( ?* ) ((疑問詞 文末 ^疑問)) > ) ( ?* ) 疑問 )


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;;; モダリティ
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;;;;; 意志，勧誘

; 書こう
; 書こうか

;;; 勧誘

; 書こう(よ|ね)
; 書きませんか

;;; 意志

; 書くつもりだ
; 書くつもりではない
; 書く気だ
; 書くつもりはない
; 書く気はない
; 書く気ではない
; 書いてみる
; 書いてみます


; 「書こう」「書こうか」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * (意志形 省略意志形) ^(できる 出来る) ((^可能動詞))] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-意志 モダリティ-勧誘 )

; 「書こう(よ|ね)」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * (意志形 省略意志形)] [助詞 * * * (よ ね ぜ)] ?*) ((文末ｏｒ補文)) > ) ( ?* ) 
  ^モダリティ-意志 ) ; モダリティ-勧誘だけに

; 「書きませんか」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * 基本連用形] 
		  [接尾辞 動詞性接尾辞 動詞性接尾辞ます型 未然形 ます] [助動詞 * 助動詞ぬ型 音便基本形 ぬ] 
		  [助詞 * * * か] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  ^モダリティ-意志 ) ; モダリティ-勧誘だけに


; 「書くつもりだ」「書くつもりではない」 
( ( ?* ) ( < ( ?* [(動詞 接尾辞)] [助動詞 * * * つもりだ] ?* ) ((文末ｏｒ補文)) > ) ( ?* )  
  モダリティ-意志 )

; 「書く気だ」
( ( ?* ) ( < ( ?* [動詞] ) > ) ( < ([名詞 * * * 気] [判定詞] ?* ) ((文末ｏｒ補文)) > ) 
  モダリティ-意志 )

; 「書くつもりはない」「書く気はない」「書く気ではない」
( ( ?* ) ( < ( ?* [動詞] ) > ) ( < ([名詞 * * * (つもり 気)] [判定詞]* [助詞 * * * は] ?* ) > < ([形容詞 * * * ない] ?* ) ((文末ｏｒ補文)) > ) 
  モダリティ-意志 )

; 「書いてみる」「書いてみます」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * タ系連用テ形] 
		  [接尾辞 * * * みる] [接尾辞 * * * ます]* ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-意志 )


;;;;; 命令

; しろ(よ)
; しなさい
; してごらん
; するように

; 「しろ(よ)」「しなさい」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * 命令形] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-命令  )

; 「してごらん」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * タ系連用テ形] [名詞 * * * ごらん] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-命令 )

; 「するように」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * 基本形] [助動詞 * * ダ列基本連用形 ようだ] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-命令 )


; ※ 「するんだ」は文脈によるので保留


;;;;; 禁止

; 書くな
; 書いては(いけない|だめだ)
; 書いてはならない
; 書くのはだめだ

; 「書くな」
( ( ?* ) ( < ( ?* [(動詞 接尾辞) * * 基本形] [助詞 終助詞 * * な] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-禁止 )

; 「書いては(いけない|だめだ)」
( ( ?* ) ( < ( ?* [動詞 * * タ系連用テ形] [助詞 * * * は] 
		  [* * * * (いけない いける 駄目だ だめだ)] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-禁止 )

; 「書いてはならない」
( ( ?* ) ( < ( ?* [動詞 * * タ系連用テ形] [助詞 * * * は] 
		  [動詞 * * * なる] [接尾辞 * * * ます]* [(接尾辞 助動詞) * * * (ない ぬ)] ?* ) ((文末ｏｒ補文)) > ) ( ?* ) 
  モダリティ-禁止 )

; 「書くのはだめだ」 ★要修正
( ( ?* ) ( < ( ?* [(動詞 接尾辞)] [名詞 * * * の] [助詞 * * * は] ) > ) 
  ( < ([形容詞 * * * (駄目だ だめだ)] ?* ) ((文末ｏｒ補文)) > ?* ) 
  モダリティ-禁止 )


;;;;; 評価

;;; モダリティ-評価:弱
; 書いて(いい|よい|よろしい)
; 書いてもいい
; 書くといい
; 書けばいい
; 書いたらいい
; 書くならいい
; 書くならばいい
; 書くのだったらいい
; 書いたっていい
; 書く方がいい
; 書く方がましだ
; 書いてもかまわない
; 書いてもかまいません
; 書けば？
; 書いたら？
; 書くことはない
; 書くことない
; 書くまでもない
; 書くにも及ばない
; 書かなくていい

;;; モダリティ-評価:強
