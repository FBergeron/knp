;;; 役職を探す (２語目以降)

( ( ? ?* )
  ( [(名詞 接尾辞) * * * ^(流し わき 本院 センター 工 医 守 首脳 げいこ 女優) 
	((&意味素:人＜職業・地位・役割＞))] )
  ( ?* )
	T役職 )

( ( ? ?* )
  ( [* * * * (副総理 副総裁)] ) ( ?* )
  T役職 )

( ( [* * * * * ((^T役職))]* )
  ( [* * * * * ((T役職))] )
  ( ?* )
	役職 )

;( ( [* * * * * ((^&意味素:人＜職業・地位・役割＞))]* )
;  ( [* * * * (院内)] [* * * * (総務)] )
