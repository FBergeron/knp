;; 役職・役職

( ( [* * * * * ((^T役職))]* 
    [* * * * * ((役職))] 
    [* * * * ・] )
    ( [* * * * * ((T役職))] )
    ( ?* )
    役職 )

;;; 役職の修飾を探す

( ( ?* ) 
  ( [* * * * (前 元 現 新 旧 副 名誉 最高 報道 編集 有力 所属 総 技術 客演 音楽 院内
	      特別 全権)] ) 
  ( [* * * * * ((役職))] ?* )
  役職修飾 ^複固)

;;; 普通の名詞

( ( ?* ) ( [* * * * (情報 所属 放送 通信 同) ] ) ( ?* )
  普通名詞)

