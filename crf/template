# Unigram

# 0 見出し
# 1 品詞
# 2 品詞細分類
# 3 文字種
# 4 文字長
# 5 形態素特徴
# 6 カテゴリ
# 7 格
# 8 親
# 9 主辞
# A 文節内位置
# B キャッシュ

# 見出し
U00:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
U04:%x[2,0]
U05:%x[-2,0]/%x[-1,0]
U06:%x[-1,0]/%x[0,0]
U07:%x[0,0]/%x[1,0]
U08:%x[1,0]/%x[2,0]
U09:%x[-1,0]/%x[0,0]/%x[1,0]

# 品詞
U10:%x[-2,1]
U11:%x[-1,1]
U12:%x[0,1]
U13:%x[1,1]
U14:%x[2,1]
U15:%x[-2,1]/%x[-1,1]
U16:%x[-1,1]/%x[0,1]
U17:%x[0,1]/%x[1,1]
U18:%x[1,1]/%x[2,1]
U19:%x[-1,1]/%x[0,1]/%x[1,1]

# 品詞細分類
U20:%x[-2,2]
