U054:%x[2,0]/%x[2,5]

# 見出し-カテゴリ
U060:%x[-2,0]/%x[-2,6]
U061:%x[-1,0]/%x[-1,6]
U062:%x[0,0]/%x[0,6]
U063:%x[1,0]/%x[1,6]
U064:%x[2,0]/%x[2,6]

# 見出し-キャッシュ
U0B0:%x[-2,0]/%x[-2,11]
U0B1:%x[-1,0]/%x[-1,11]
U0B2:%x[0,0]/%x[0,11]
U0B3:%x[1,0]/%x[1,11]
U0B4:%x[2,0]/%x[2,11]

# 品詞-主辞
U190:%x[-2,1]/%x[-2,9]
U191:%x[-1,1]/%x[-1,9]
U192:%x[0,1]/%x[0,9]
U193:%x[1,1]/%x[1,9]
U194:%x[2,1]/%x[2,9]

