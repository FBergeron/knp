UA4:%x[2,10]

# キャッシュ
UB0:%x[-2,11]
UB1:%x[-1,11]
UB2:%x[0,11]
UB3:%x[1,11]
UB4:%x[2,11]

# 見出し-品詞
U010:%x[-2,0]/%x[-2,1]
U011:%x[-1,0]/%x[-1,1]
U012:%x[0,0]/%x[0,1]
U013:%x[1,0]/%x[1,1]
U014:%x[2,0]/%x[2,1]

# 見出し-品詞細分類
U020:%x[-2,0]/%x[-2,2]
U021:%x[-1,0]/%x[-1,2]
U022:%x[0,0]/%x[0,2]
U023:%x[1,0]/%x[1,2]
U024:%x[2,0]/%x[2,2]

# 見出し-文字種
U030:%x[-2,0]/%x[-2,3]
U031:%x[-1,0]/%x[-1,3]
U032:%x[0,0]/%x[0,3]
U033:%x[1,0]/%x[1,3]
U034:%x[2,0]/%x[2,3]

# 見出し-文字長
U040:%x[-2,0]/%x[-2,4]
U041:%x[-1,0]/%x[-1,4]
U042:%x[0,0]/%x[0,4]
U043:%x[1,0]/%x[1,4]
U044:%x[2,0]/%x[2,4]

# 見出し-形態素特徴
U050:%x[-2,0]/%x[-2,5]
U051:%x[-1,0]/%x[-1,5]
U052:%x[0,0]/%x[0,5]
